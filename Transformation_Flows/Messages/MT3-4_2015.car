							<SubField Name="Currency">
								<Validation>
									<Options>B</Options>
									<ErrorCode>T52</ErrorCode>
									<Comment><![CDATA[Currency must be a valid ISO 4217 currency code (Error code(s): T52).]]></Comment>
									<ValidationPattern xsi:type="Currency_Code" Type="Currency Code">
