		<validationrules name="Default">
			<hdrvalidationrules>
				<fieldvalidations>
					<fieldvalidation>
						<name>U09</name>
						<formula><![CDATA[if($this.Header.GetFieldIndex("User_Header") != -1) {
    def userHeader = $this.Header.GetSection("User_Header");
    if(userHeader.$size > 0 && userHeader[0].IsNotNull("Validation_Flag")) {
        return false;
    }
}
return true;
]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>U09</error-code>
						<actionmessage><![CDATA["User has used tag 119 in a message that is not a candidate for special processing"]]></actionmessage>
					</fieldvalidation>
				</fieldvalidations>
			</hdrvalidationrules>
			<datavalidationrules>
				<fieldvalidations>
					<fieldvalidation>
						<name>FC-18A-A</name>
						<fieldname>C.18A.Number</fieldname>
						<formula><![CDATA[$value == SecCount(C.C1, "F30F")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D96</error-code>
						<actionmessage><![CDATA["In Field 18A of sequence C, Number must equal the number of occurrences of the subsequent field 30F."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>FE-18A-A</name>
						<fieldname>E.18A.Number</fieldname>
						<formula><![CDATA[$value == SecCount(E.E1, "F30F")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D96</error-code>
						<actionmessage><![CDATA["In Field 18A of sequence E, Number must equal the number of occurrences of the subsequent field 30F."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>FA-22C</name>
						<fieldname>A.22C.Reference_Code</fieldname>
						<formula><![CDATA[$value == Mid(A[0].F30P[0].Date, 2, 4)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>T22</error-code>
						<actionmessage><![CDATA["In field 22C (in sequence A), Location Code 1 and Bank Code 2 are separated by four digits, which must consist of the year (its last two characters) and month of the termination date in field 30P, in the format YYMM."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C1</name>
						<formula><![CDATA[B.$size > 0 || D.$size > 0]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E47</error-code>
						<actionmessage><![CDATA["At least one of the sequences B or D must be present."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C3</name>
						<formula><![CDATA[If(A[0].F23A[0].Settlement_Method == "NET", 
   If(C.$size > 0, E.$size == 0, E.$size > 0), 
   true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E49</error-code>
						<actionmessage><![CDATA["When the settlement method, ie, the second subfield of field 23A, is 'NET', either sequence C or E must be present, but not both."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C2</name>
						<formula><![CDATA[C.$size > 0 || E.$size > 0]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E48</error-code>
						<actionmessage><![CDATA["At least one of the sequences C or E must be present."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C4</name>
						<fieldname>A.23A.Settlement_Method</fieldname>
						<formula><![CDATA[If($value == "NET", 
   If(C.$size > 0, 
      C[0].F18A[0].Number == 1, 
      E.$size > 0 && E[0].F18A[0].Number == 1), 
   true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E50</error-code>
						<actionmessage><![CDATA["When the settlement method, ie, the second subfield of field 23A, is 'NET', the payments block (i.e. fields 30F---57a) within sequence C or E must occur only once. i.e. field 18A in seq. C or E must be =1."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C5</name>
						<fieldname>A.23A.Settlement_Method</fieldname>
						<formula><![CDATA[If($value == "GROSS", 
   If(C.$size > 0, C[0].F18A[0].Number < 4, true) && 
     If(E.$size > 0, E[0].F18A[0].Number < 4, true), 
   true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E51</error-code>
						<actionmessage><![CDATA["When the settlement method, ie, the second subfield of field 23A, is 'GROSS', the payments block (i.e. fields 30F---57a) within sequence C or E cannot occur more than three times. i.e field 18A must be <4."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C6-A</name>
						<fieldname>B.32H.Currency</fieldname>
						<formula><![CDATA[$value == B[0].F33F[0].Currency]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E38</error-code>
						<actionmessage><![CDATA["The currency code of the fields 33F and 32H in sequence B must be the same."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C6-B</name>
						<fieldname>D.32H.Currency</fieldname>
						<formula><![CDATA[$value == D[0].F33F[0].Currency]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E38</error-code>
						<actionmessage><![CDATA["The currency codes of the fields 33F and 32H in sequence D must be the same."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C7-A</name>
						<fieldname>C.C1.86a</fieldname>
						<formula><![CDATA[C.C1.F56a.$size > 0]]></formula>
