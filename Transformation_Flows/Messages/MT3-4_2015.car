						<formula><![CDATA[If(In($value, "AMND", "CANC"), A[0].F21.$size > 0, true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D02</error-code>
						<actionmessage><![CDATA["In sequence A, if field 22A is AMND or CANC, then field 21 is mandatory, otherwise it is optional."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C2</name>
						<fieldname>A.94A.Scope</fieldname>
						<formula><![CDATA[If($value == "ASET", 
   A[0].F17O.$size == 0 && A[0].F17N.$size == 0, 
   If($value == "AFWD", 
      A[0].F17O.$size > 0 && A[0].F17N.$size > 0, 
      true))]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D03</error-code>
						<actionmessage><![CDATA["In sequence A, if field 94A is ASET, then fields 17O and 17N are not allowed; if field 94A is AFWD, then fields 17O and 17N are mandatory."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C3-A</name>
						<fieldname>A.17O.Indicator</fieldname>
						<formula><![CDATA[If($value == "Y", 
   A[0].F17F.$size == 0, 
   If($value == "N", A[0].F17F.$size > 0, true))]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D04</error-code>
						<actionmessage><![CDATA["In sequence A, if field 17O is Y, then field 17F is not allowed; if field 17O is N, then field 17F is mandatory."]]></actionmessage>
					</fieldvalidation>
