									<SubField Name="Indicator">
										<Validation>
											<Options>X</Options>
											<ErrorCode>T36</ErrorCode>
											<Comment><![CDATA[Indicator must contain one of the following codes (Error code(s): T36):]]></Comment>
											<ValidationPattern xsi:type="CODE" Type="CODE">
												<Type>CODE</Type>
												<Codes>
													<Code Name="N">
														<Description>No, the contract is not directly linked to the non-financial counterparty's commercial or treasury financing activity.</Description>
													</Code>
													<Code Name="Y">
														<Description>Yes, the contract is directly linked to the non-financial counterparty's commercial or treasury financing activity.</Description>
													</Code>
												</Codes>
											</ValidationPattern>
										</Validation>
									</SubField>
								</Validations>
							</SwiftField>
							<SwiftField Name="77A">
								<SwiftFieldDef Name="77">
									<Generic>false</Generic>
									<Option Name="A">
										<Format><![CDATA[20*35x]]></Format>
										<Specification><![CDATA[(Narrative)]]></Specification>
										<Description><![CDATA[Additional Reporting Information ]]></Description>
									</Option>
