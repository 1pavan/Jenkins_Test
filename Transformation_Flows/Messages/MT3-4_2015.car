						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>D56</error-code>
						<actionmessage><![CDATA["In sequence B, field 32H is not allowed and field 30X is mandatory, if field 22B (in sequence A) contains CONF."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>C3-B</name>
						<fieldname>A.22B.Type</fieldname>
