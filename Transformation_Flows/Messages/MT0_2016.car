]]></description>
								<datatype>String</datatype>
								<required>true</required>
								<length>4</length>
							</field>
							<field>
								<name>Message Priority</name>
								<datatype>String</datatype>
								<required>false</required>
								<length>1</length>
							</field>
						</fields>
					</field>
				</fields>
