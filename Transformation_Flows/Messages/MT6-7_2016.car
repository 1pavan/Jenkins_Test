					<SwiftField Name="72">
						<SwiftFieldDef Name="72">
							<Generic>false</Generic>
							<Description><![CDATA[Sender to Receiver Information]]></Description>
							<Option Name="">
								<Format><![CDATA[6*35x]]></Format>
								<Specification><![CDATA[(Narrative)]]></Specification>
								<Description><![CDATA[Sender to Receiver Information]]></Description>
							</Option>
						</SwiftFieldDef>
