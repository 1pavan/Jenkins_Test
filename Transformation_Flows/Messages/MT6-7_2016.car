						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>C55</error-code>
						<actionmessage><![CDATA["In any sequence B, the currency code in fields 33B and 34a must be the same"]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>ERI</name>
